{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "rules": {
    "import/no-extraneous-dependencies": ["error", { "devDependencies": ["**/__tests__/*", "**/*.test.js", "**/*.spec.js"]}],
    "react/jsx-filename-extension": 0
  },
  "plugins": [
    "react",
    "import"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true,
    "node": true
  }
}
